<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/nomeApp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/app_name"
        android:textColor="#BD0B0B"
        android:textSize="38sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.25" /><![CDATA[

    />



    ]]>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/botao_google"
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        android:text="Login Google"
        android:textColor="#000000"
        android:textSize="16sp"
        app:backgroundTint="#A8C4D1"
        app:cornerRadius="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.448"
        app:strokeColor="#000000"
        app:strokeWidth="1dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/botao_criar_conta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/criar_conta"
        android:textColor="#000000"
        android:textSize="12sp"
        app:backgroundTint="#A8C4D1"
        app:cornerRadius="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.944"
        app:strokeColor="#000000"
        app:strokeWidth="1dp" /><![CDATA[

    />



    ]]>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/botao_facebook"
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        android:text="Login Facebook"
        android:textColor="#000000"
        android:textSize="16sp"
        app:backgroundTint="#A8C4D1"
        app:cornerRadius="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.535"
        app:strokeColor="#000000"
        app:strokeWidth="1dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/botao_login_email"
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        android:text="@string/login_email"
        android:textColor="#000000"
        android:textSize="16sp"
        app:backgroundTint="#A8C4D1"
        app:cornerRadius="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.625"
        app:strokeColor="#000000"
        app:strokeWidth="1dp" />/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/botao_desenvolvedor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:shadowColor="#00FFFFFF"
        android:text="Desenvolvedor -->"
        android:textColor="#000000"
        app:backgroundTint="#00A8C4D1"
        app:elevation="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.842"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.783" />
</androidx.constraintlayout.widget.ConstraintLayout>